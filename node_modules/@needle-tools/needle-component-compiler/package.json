{
    "name": "@needle-tools/needle-component-compiler",
    "version": "1.11.2",
    "description": "Compile mock unity components from typescript",
    "main": "index.js",
    "scripts": {
        "tsc": "tsc",
        "dev": "npm-watch compile",
        "compile" : "tsc",
        "test": "mocha -r ts-node/register test/**.test.ts"
    },
    "dependencies": {
        "typescript": "^4.5.5"
    },
    "devDependencies": {
        "@types/chai": "^4.3.3",
        "@types/mocha": "^9.1.1",
        "@types/node": "^18.7.18",
        "chai": "^4.3.6",
        "mocha": "^10.0.0",
        "ts-node": "^10.9.1",
        "npm-watch": "^0.13.0"
    },
    "watch": {
      "compile": {
        "patterns": [
          "src/component-compiler.ts"
        ],
        "extensions": "ts",
        "quiet": false
      }
    },
    "author": {
        "name": "Needle",
        "email": "help@needle.tools",
        "url": "https://needle.tools/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/needle-tools/needle-tiny-component-compiler.git"
    }
}
